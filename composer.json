{
    "name": "ziiframework/zii",
    "description": "zii framework dev",
    "keywords": [
        "zii",
        "framework"
    ],
    "homepage": "https://github.com/ziiframework/zii",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "support": {
        "issues": "https://github.com/ziiframework/zii/issues?state=open",
        "source": "https://github.com/ziiframework/zii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3.0",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "lib-pcre": "*",
        "ext-json": "*",
        "ziiframework/composer": "3.0.x-dev",
        "ezyang/htmlpurifier": "~4.6",
        "cebe/markdown": "~1.0.0 | ~1.1.0 | ~1.2.0",
        "bower-asset/jquery": "3.5.*@stable | 3.4.*@stable | 3.3.*@stable | 3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
        "bower-asset/inputmask": "~3.2.2 | ~3.3.5",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "~2.0.1"
    },
    "require-dev": {
        "cweagans/composer-patches": "^1.6",
        "phpunit/phpunit": "^8.5",
        "cebe/indent": "~1.0.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\": "framework/"
        }
    },
    "config": {
        "platform": {"php": "7.3"}
    },
    "bin": [
        "framework/yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
